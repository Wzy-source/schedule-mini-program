
page {
	background-color: #f0f0f0;
}
.page {
	width: 100%;
	padding: 32rpx;
	box-sizing: border-box;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.logo {
	-webkit-align-self: center;
	        align-self: center;
	width: 120rpx;
	height: 120rpx;
	border-radius: 32rpx;
	margin-top: 160rpx;
}
.logo > image{
	width: 100%;
	height: 100%;
	border-radius: 32rpx;
}
.name {
	-webkit-align-self: center;
	        align-self: center;
	font-size: 28rpx;
	color: #8F8F94;
	margin-top: 24rpx;
	font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
}
.btn {
	-webkit-align-self: center;
	        align-self: center;
	margin-top: 100rpx;
	width: 70%;
	height: 80rpx;
}
.btn>button {
	width: 100%;
	font-size: 36rpx;
	height: 100%;
	font-weight: 300;
	font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
	box-shadow: 0px 6rpx 6rpx rgba(0, 0, 0, 0.3);
}

